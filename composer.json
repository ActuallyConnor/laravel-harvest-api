{
    "name": "actuallyconnor/laravel-harvest-api",
    "description": "Laravel Harvest API",
    "keywords": [
        "ActuallyConnor",
        "laravel",
        "laravel-harvest-api"
    ],
    "homepage": "https://github.com/actuallyconnor/laravel-harvest-api",
    "license": "MIT",
    "authors": [
        {
            "name": "Connor Smyth",
            "email": "me@connorsmyth.com",
            "homepage": "https://connorsmyth.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "spatie/laravel-package-tools": "^1.4.3",
        "illuminate/contracts": "^8.37",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "brianium/paratest": "^6.2",
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "phpunit/phpunit": "^9.3",
        "spatie/laravel-ray": "^1.9",
        "vimeo/psalm": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "Actuallyconnor\\LaravelHarvestApi\\": "src",
            "Actuallyconnor\\LaravelHarvestApi\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Actuallyconnor\\LaravelHarvestApi\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "./vendor/bin/testbench package:test --parallel --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Actuallyconnor\\LaravelHarvestApi\\LaravelHarvestApiServiceProvider"
            ],
            "aliases": {
                "LaravelHarvestApi": "Actuallyconnor\\LaravelHarvestApi\\LaravelHarvestApiFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
